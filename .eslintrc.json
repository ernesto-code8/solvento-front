{
    "env": {
        "browser": true,
        "es2021": true
    },
    "settings": {
        "react":{
            "version":"detect"
        }
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "warn",
        "@typescript-eslint/no-unused-vars": "warn"
    }
}